{
  "version": "0.2.0",
  "configurations": [
    
      {
          "type": "node",
          "request": "launch",
          "name": "Launch Program",
          "skipFiles": [
              "<node_internals>/**"
          ],
          "program": "${workspaceFolder}//server//server.js",
          "outFiles": ["${workspaceFolder}/**/*.js"],
          "cwd": "${workspaceRoot}",
          "sourceMaps": true,
          "console": "integratedTerminal",
          "protocol": "inspector",
          "timeout": 30000
      }
  ]
}